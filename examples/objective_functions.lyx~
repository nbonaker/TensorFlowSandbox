\begin_body

\begin_layout Standard
Some objective functions you might play with.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \textrm{Quadratic}\\
\theta & \in\mathbb{R}^{D}\\
f\left(\theta\right) & =\frac{1}{2}\theta^{T}A\theta+b^{T}\theta
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \textrm{Normal log likelihood}\\
\mu,\sigma,x & \in\mathbb{R}\\
\ell\left(x\vert\mu,\sigma\right) & =-\frac{1}{2}\sigma^{-2}\left(x-\mu\right)^{2}-\frac{1}{2}\log\sigma^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \textrm{Normal influence function}\\
\mu,\sigma & \in\mathbb{R}\\
x,w & \in\mathbb{R}^{N}\\
f\left(\mu,\sigma,x,w\right) & =\sum_{n}w_{n}\ell\left(x_{n}\vert\mu,\sigma\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \textrm{EM mixture model}\\
x & \in\mathbb{R}^{N}\\
\mu_{1},...,\mu_{K},\sigma_{1},...,\sigma_{K} & \in\mathbb{R}\\
\pi & \in\left[0,1\right]^{K}\\
z_{n} & \in\left[0,1\right]^{K}\textrm{ (the interval, not binaries)}\\
f\left(x,\mu_{1},...,\mu_{K},\sigma_{1},...,\sigma_{K},z\right) & =\sum_{n}\sum_{k}z_{nk}\ell\left(x_{n}\vert\mu_{k},\sigma_{k}\right)+\sum_{n}\sum_{k}z_{nk}\log\pi_{k}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
 & \textrm{Logistic regression}\\
y & \in\left\{ 0,1\right\} ^{N}\textrm{ (binary)}\\
x_{n} & \in\mathbb{R}^{D}\\
x & =x_{1},...,x_{N}\\
\theta & \in\mathbb{R}^{D}\\
\ell\left(y\vert\theta,x\right) & =\sum_{n}\left(y_{n}\left(\theta^{T}x_{n}\right)+\log\left(1+\exp\left(\theta^{T}x_{n}\right)\right)\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard

\end_layout

